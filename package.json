{
  "name": "redux-playground",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "deploy": "bun run build && wrangler pages deploy ./build/client --project-name simple-typing",
    "lint": "bunx @biomejs/biome lint ./app",
    "start": "remix-serve ./build/server/index.js",
    "test": "vitest",
    "test:update": "vitest -u",
    "coverage": "vitest run --coverage",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.2",
    "@remix-run/cloudflare": "^2.8.1",
    "@remix-run/cloudflare-pages": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@vanilla-extract/css": "^1.14.1",
    "@vanilla-extract/dynamic": "^2.1.0",
    "isbot": "^4.1.0",
    "miniflare": "^3.20231030.4",
    "random-words": "^2.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.3",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@happy-dom/global-registrator": "^14.3.9",
    "@remix-run/dev": "^2.8.1",
    "@testing-library/react": "^14.2.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vanilla-extract/vite-plugin": "^4.0.6",
    "@vitest/coverage-v8": "^1.4.0",
    "bun-types": "^1.0.36",
    "happy-dom": "^14.3.9",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.4.0",
    "wrangler": "^3.41.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
